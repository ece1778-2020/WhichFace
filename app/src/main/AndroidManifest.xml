<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spiral2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:parentActivityName=".MainActivity" android:label="Upload" android:name=".Display"></activity>
        <activity android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:parentActivityName=".MainActivity" android:label="Test"  android:name=".TestActivity" />
        <activity android:parentActivityName=".MainActivity" android:label="Test" android:name=".TestUnitActivity" />
        <activity android:parentActivityName=".MainActivity" android:label="Result" android:name=".ResultActivity" />
        <activity android:parentActivityName=".MainActivity" android:label="Gallery" android:name=".PhotoLibraryActivity" />
        <activity android:parentActivityName=".MainActivity" android:label="Accuracy" android:name=".AccuracyActivity" />
    </application>

</manifest>